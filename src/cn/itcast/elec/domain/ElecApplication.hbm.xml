<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.elec.domain.ElecApplication" table="Elec_Application">
		<id name="applicationID" type="long">
			<column name="applicationID"></column>
			<generator class="increment"></generator>
		</id>
		<property name="applicationTemplateID" type="long">
			<column name="applicationTemplateID"></column>
		</property>
		<property name="title" type="string">
			<column name="title"></column>
		</property>
		<property name="path" type="string">
			<column name="path"></column>
		</property>
		<property name="applicationUserID" type="string">
			<column name="applicationUserID"></column>
		</property>
		<property name="applicationLogonName" type="string">
			<column name="applicationLogonName"></column>
		</property>
		<property name="applicationUserName" type="string">
			<column name="applicationUserName"></column>
		</property>
		<property name="applyTime" type="timestamp">
			<column name="applyTime"></column>
		</property>
		<property name="status" type="string">
			<column name="status"></column>
		</property>
	</class>
</hibernate-mapping>

