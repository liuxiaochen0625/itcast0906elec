<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.elec.domain.ElecCommonMsg" table="Elec_CommonMsg">
		<id name="comID" type="string">
			<column name="comID"></column>
			<generator class="uuid"></generator>
		</id>
		<property name="stationRun" type="string">
			<column name="stationRun" sql-type="VARCHAR(5000)"></column>
		</property>
		<property name="devRun" type="string">
			<column name="devRun" sql-type="VARCHAR(5000)"></column>
		</property>
		<property name="createDate" type="timestamp">
			<column name="createDate" length="50"></column>
		</property>
	</class>
</hibernate-mapping>

