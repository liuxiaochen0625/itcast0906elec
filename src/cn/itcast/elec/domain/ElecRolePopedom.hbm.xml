<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.itcast.elec.domain.ElecRolePopedom" table="Elec_Role_Popedom">
		<id name="roleID" type="string">
			<column name="roleID"></column>
		</id>
		<property name="popedomcode" type="string">
			<column name="popedomcode" sql-type="varchar(500)"></column>
		</property>
		<property name="remark" type="string">
			<column name="remark"></column>
		</property>
	</class>
</hibernate-mapping>

